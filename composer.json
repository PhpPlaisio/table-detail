{
  "name": "plaisio/table-detail",
  "description": "PhpPlaisio: Detail Table",
  "keywords": [
    "PhpPlaisio",
    "Detail Table"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/babel": "^2.1.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/kernel": "^3.2.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "setbased/error-handler": "1.3.0",
    "plaisio/console-kernel": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Table\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Table\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
