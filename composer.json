{
  "name": "plaisio/table-detail",
  "description": "PhpPlaisio: Detail Table",
  "keywords": [
    "PhpPlaisio",
    "Detail Table"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/babel": "^2.2.0",
    "plaisio/exception": "^1.3.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/kernel": "^3.3.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "setbased/error-handler": "^1.4.0",
    "plaisio/console-kernel": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Table\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Table\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
