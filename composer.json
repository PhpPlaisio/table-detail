{
  "name": "plaisio/table-detail",
  "description": "PhpPlaisio: Detail Table",
  "keywords": [
    "PhpPlaisio",
    "Detail Table"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "plaisio/babel": "^2.0.1",
    "plaisio/exception": "^1.1.0",
    "plaisio/helper-html": "^4.0.0",
    "plaisio/kernel": "^3.1.1"
  },
  "require-dev": {
    "phing/phing": "^2.17.0",
    "phpunit/phpunit": "^9.5.11",
    "plaisio/console": "^2.5.0."
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Table\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Table\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
